@model KlopZavod.Models.Khojagi

@{
    ViewData["Title"] = "Подробно";
}

<h1>Подробно</h1>

<div>
    <h4>Дехкансокое хозяйство</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Наименование
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            Директор
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Director)
        </dd>
        <dt class = "col-sm-2">
            ИНН
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RMA)
        </dd>
        <dt class = "col-sm-2">
            Телефон
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Phone)
        </dd>
        <dt class = "col-sm-2">
            Гектар(га)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Ga)
        </dd>
        <dt class = "col-sm-2">
            Ассоциация
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Assoc.Name)
        </dd>
        <dt class = "col-sm-2">
            Район
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Rayon.Name)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.KhojagiID">Изменить</a> |
    <a asp-action="Index">Назад к списку</a>
</div>
