@model KlopZavod.Models.Partiya

@{
    ViewData["Title"] = "Подробно";
}

    <h1>Подробно</h1>

<div>
    <h4>Партия</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            №
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomerPart)
        </dd>
        <dt class = "col-sm-2">
            Вид
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Vid)
        </dd>
        <dt class = "col-sm-2">
            Сорт
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sort)
        </dd>
        <dt class = "col-sm-2">
            Вид сбора
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sbor)
        </dd>
        <dt class = "col-sm-2">
            Бунт
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bunt.Name)
        </dd>
        <dt class = "col-sm-2">
            Разновидность
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Semena.Name)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.PartiyaID">Изменить</a> |
    <a asp-action="Index">Назад к списку</a>
</div>
