@model KlopZavod.Models.Partiya

@{
    ViewData["Title"] = "Delete";
}

<h1>Удаление</h1>

<h3>Вы уверены, что хотите удалить запись?</h3>
<div>
    <h4>Партия</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            №
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomerPart)
        </dd>
        <dt class = "col-sm-2">
            Вид
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Vid)
        </dd>
        <dt class = "col-sm-2">
            Сорт
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sort)
        </dd>
        <dt class = "col-sm-2">
            Вид сбора
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sbor)
        </dd>
        <dt class = "col-sm-2">
            Бунт
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bunt.Name)
        </dd class>
        <dt class = "col-sm-2">
            Разновидность
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Semena.Name)
        </dd class>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="PartiyaID" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">Назад к списку</a>
    </form>
</div>
