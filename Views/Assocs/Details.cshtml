@model KlopZavod.Models.Assoc

@{
    ViewData["Title"] = "Подробно";
}

<h1>Подробно</h1>

<div>
    <h4>Ассоциация</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Наименование
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            Наименование(сокр.)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SocrName)
        </dd>
        <dt class = "col-sm-2">
            Джамоат
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Jamoat)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.AssocID">Изменить</a> |
    <a asp-action="Index">Назад к списку</a>
</div>
